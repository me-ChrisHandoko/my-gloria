  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:seed": "ts-node src/database/seeders/permission.seeder.ts",
    "db:seed:migrate": "ts-node prisma/seeds/migrate-existing-permissions.ts",
    "db:setup": "npm run db:migrate:dev && npm run db:seed",
    "permissions:seed": "ts-node src/database/seeders/permission.seeder.ts",
    "permissions:migrate": "ts-node prisma/seeds/migrate-existing-permissions.ts",
    "rls:setup": "ts-node scripts/setup-rls.ts",
    "rls:apply": "prisma db execute --file prisma/migrations/manual/001_enable_rls_policies.sql",
    "rls:validate": "ts-node scripts/validate-rls.ts",
    "rls:test": "jest src/security/rls.spec.ts",
    "rls:test-user": "ts-node scripts/test-rls-user.ts",
    "rls:enable": "ts-node scripts/rls-control.ts enable",
    "rls:disable": "ts-node scripts/rls-control.ts disable",
    "rls:status": "ts-node scripts/rls-control.ts status",
    "notification:migrate-templates": "ts-node scripts/migrate-notification-templates.ts"
  },
